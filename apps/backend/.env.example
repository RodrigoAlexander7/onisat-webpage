# Database connection string
# Format: postgresql://user:password@host:port/database?schema=public
# For local development
#DATABASE_URL=postgresql://postgres:yourpassword@localhost:5432/your_database?schema=public
# For Docker Compose
DATABASE_URL=postgresql://postgres:postgres@postgresql:5432/testdb?schema=public

# Authentication secret key
# Generate with: openssl rand -hex 32
AUTH_SECRET=your_secret_key_here

# Google OAuth credentials
# Get from: https://console.cloud.google.com/
AUTH_GOOGLE_ID=your_google_client_id.apps.googleusercontent.com
AUTH_GOOGLE_SECRET=your_google_client_secret

# Google OAuth callback URL
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Frontend application URL
FRONTEND_URL=http://localhost:4000

# Server port
PORT=3000

# Node environment (development, production, test)
NODE_ENV=development

EMAIL_WHITELIST=rfernandezh@unsa.edu.pe:rodrygoleu7@gmail.com

